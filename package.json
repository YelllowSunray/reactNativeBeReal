  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "npx expo export --platform web && npm run copy-seo",
    "copy-seo": "cp web/manifest.json dist/manifest.json && cp web/robots.txt dist/robots.txt && cp web/sitemap.xml dist/sitemap.xml",
    "deploy": "npx expo export --platform web && npm run copy-seo && vercel --prod"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "expo": "^54.0.12",
    "expo-audio": "~1.0.13",
    "expo-camera": "~17.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-media-library": "~18.2.0",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "^6.0.7",
    "expo-video": "~3.0.11",
    "firebase": "^12.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-webcam": "^7.2.0"
  },
  "private": true,
  "devDependencies": {
    "@react-native-community/cli": "^20.0.2",
    "babel-preset-expo": "^54.0.4"
  }
